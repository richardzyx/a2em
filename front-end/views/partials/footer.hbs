<!-- jQuery -->

<script src="static/js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="static/js/bootstrap.min.js"></script>

 <!-- Morris Charts JavaScript -->
    <script src="static/js/plugins/morris/raphael.min.js"></script>
    <script src="static/js/plugins/morris/morris.min.js"></script>
    <script src="static/js/plugins/morris/morris-data.js"></script>

<script>
	var drawGraph = function(){ new Morris.Line({
	  // ID of the element in which to draw the chart.
	  element: 'myfirstchart',
	  // Chart data records -- each entry in this array corresponds to a point on
	  // the chart.
	  data: [
	    { day: {{week.0.day}} , amount: {{week.0.amount}}, numberofDonations: {{week.0.num}} },
		{ day: {{week.1.day}} , amount: {{week.1.amount}}, numberofDonations: {{week.1.num}} },
		{ day: {{week.2.day}} , amount: {{week.2.amount}}, numberofDonations: {{week.2.num}} },
		{ day: {{week.3.day}} , amount: {{week.3.amount}}, numberofDonations: {{week.3.num}} },
		{ day: {{week.4.day}} , amount: {{week.4.amount}}, numberofDonations: {{week.4.num}} },
		{ day: {{week.5.day}} , amount: {{week.5.amount}}, numberofDonations: {{week.5.num}} },
		{ day: {{week.6.day}} , amount: {{week.6.amount}}, numberofDonations: {{week.6.num}} }

	  ],
	  // The name of the data record attribute that contains x-values.
	  xkey: 'day',
	  // A list of names of data record attributes that contain y-values.
	  ykeys: ['amount', 'numberofDonations'],
	  // Labels for the ykeys -- will be displayed when you hover over the
	  // chart.
	  labels: ['Amount', 'Number of Donations']
	});}

</script>
